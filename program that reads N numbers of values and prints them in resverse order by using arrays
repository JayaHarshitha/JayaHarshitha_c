#include<stdio.h>
int main()
{
    int N;
    printf("enter the number of values (N):");
    scanf("%d",&N);
    int arr[N];
    printf("enter %d values:\n",N);
    for (int i=0;i<N;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("values in reverse order:\n");
    for(int i=N-1;i>=0;i--)
    {
        printf("%d",arr[i]);
    }
    printf("\n");
    return 0;
}
